<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="10dp">

    <TextView
        android:id="@+id/idpembayarancashierlist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/textView10"
        android:textSize="12sp"
        android:fontFamily="@font/paprika"
        android:text="id" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="12sp"
        android:fontFamily="@font/paprika"
        android:text="Id Pembayaran: " />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView10"
        android:fontFamily="@font/paprika"
        android:text="Nama Customer: "
        android:textSize="12sp" />

    <TextView
        android:id="@+id/namacustomercashierlist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/idpembayarancashierlist"
        android:layout_toRightOf="@+id/textView6"
        android:text="nama"
        android:fontFamily="@font/paprika"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/namacustomercashierlist"
        android:layout_below="@id/idpembayarancashierlist"
        android:textSize="12sp"
        android:layout_marginLeft="3dp"
        android:fontFamily="@font/paprika"
        android:text="(" />

    <TextView
        android:id="@+id/usernamecashierlist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/textView9"
        android:layout_below="@id/idpembayarancashierlist"
        android:textSize="12sp"
        android:fontFamily="@font/paprika"
        android:text="username" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/usernamecashierlist"
        android:layout_below="@id/idpembayarancashierlist"
        android:textSize="12sp"
        android:fontFamily="@font/paprika"
        android:text=")" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView6"
        android:text="Tanggal Pembayaran: "
        android:fontFamily="@font/paprika"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/tglcashierlist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/namacustomercashierlist"
        android:layout_toRightOf="@+id/textView7"
        android:text="tgl"
        android:fontFamily="@font/paprika"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView7"
        android:text="Total Pembayaran: Rp. "
        android:fontFamily="@font/paprika"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/totalpembayarancashierlist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tglcashierlist"
        android:layout_toRightOf="@+id/textView8"
        android:text="total"
        android:fontFamily="@font/paprika"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView42"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView8"
        android:fontFamily="@font/paprika"
        android:textSize="12sp"
        android:text="Metode Pembayaran: " />

    <TextView
        android:id="@+id/metodecashierlist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/totalpembayarancashierlist"
        android:layout_toRightOf="@id/textView42"
        android:fontFamily="@font/paprika"
        android:textSize="12sp"
        android:text="metode" />

</RelativeLayout>